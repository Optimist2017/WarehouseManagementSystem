<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox prefHeight="477.0" prefWidth="632.0" spacing="10.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.updateproductController">
    <children>
        <VBox alignment="CENTER">
            <children>
                <Text fill="#3d65eb" strokeType="OUTSIDE" strokeWidth="0.0" text="Update Product">
                    <font>
                        <Font size="25.0" />
                    </font>
                </Text>
            </children>
        </VBox>
        <HBox prefHeight="100.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
            <children>
                <VBox prefHeight="422.0" prefWidth="312.0" spacing="10.0">
                    <children>
                        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Product Name :" />
                        <TextField fx:id="name" />
                        <Text layoutX="10.0" layoutY="23.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Quantity :" />
                        <TextField fx:id="quantity" layoutX="10.0" layoutY="27.0" />
                        <Text layoutX="10.0" layoutY="65.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Purchase Price :" />
                        <TextField fx:id="purchaseprice" layoutX="10.0" layoutY="69.0" />
                        <Text layoutX="10.0" layoutY="106.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Sales Price :" />
                        <TextField fx:id="salesprice" layoutX="10.0" layoutY="111.0" />
                        <Text layoutX="10.0" layoutY="147.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Description :" />
                        <TextArea fx:id="description" prefHeight="96.0" prefWidth="306.0" />
                    </children>
                    <padding>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                    </padding>
                </VBox>
                <VBox prefHeight="422.0" prefWidth="313.0" spacing="10.0">
                    <children>
                        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Supplied By :" />
                        <ComboBox fx:id="suppliedby" prefHeight="25.0" prefWidth="166.0" />
                        <Text layoutX="10.0" layoutY="23.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Brand :" />
                        <ComboBox fx:id="brand" layoutX="10.0" layoutY="27.0" prefHeight="25.0" prefWidth="167.0" />
                        <Text layoutX="10.0" layoutY="65.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Category :" />
                        <ComboBox fx:id="category" layoutX="10.0" layoutY="68.0" prefHeight="25.0" prefWidth="166.0" />
                        <Text layoutX="10.0" layoutY="105.0" strokeType="OUTSIDE" strokeWidth="0.0" text="User :" />
                        <TextField fx:id="user" prefHeight="25.0" prefWidth="226.0" />
                        <Text layoutX="10.0" layoutY="148.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Date Added :" />
                        <DatePicker fx:id="dateadded" />
                    </children>
                    <padding>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                    </padding>
                </VBox>
            </children>
        </HBox>
        <VBox alignment="CENTER">
            <children>
                <Button mnemonicParsing="false" onAction="#update" text="Update" />
            </children>
        </VBox>
    </children>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
    </padding>
</VBox>
