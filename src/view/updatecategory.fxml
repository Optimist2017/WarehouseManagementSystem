<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox prefHeight="270.0" prefWidth="324.0" spacing="10.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.updatecategoryController">
    <children>
        <VBox alignment="CENTER">
            <children>
                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Update Category">
                    <font>
                        <Font size="25.0" />
                    </font>
                </Text>
            </children>
        </VBox>
        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Name :" />
        <TextField fx:id="name" />
        <Text layoutX="10.0" layoutY="57.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Description :" />
        <TextArea fx:id="description" prefHeight="83.0" prefWidth="304.0" />
        <VBox alignment="CENTER">
            <children>
                <Button mnemonicParsing="false" onAction="#update" text="Update" />
            </children>
        </VBox>
    </children>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
    </padding>
</VBox>
