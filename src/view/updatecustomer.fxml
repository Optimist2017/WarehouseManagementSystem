        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
    </padding>
    <children>
        <VBox alignment="CENTER">
            <children>
                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Update Customer">
                    <font>
                        <Font size="25.0" />
                    </font>
                </Text>
            </children>
        </VBox>
        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Customer Name :" />
        <TextField fx:id="name" />
        <Text layoutX="20.0" layoutY="77.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Customer Contact :" />
        <TextField fx:id="contact" layoutX="20.0" layoutY="91.0" />
        <Text layoutX="20.0" layoutY="139.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Customer Address :" />
        <TextArea fx:id="address" prefHeight="128.0" prefWidth="352.0" />
        <VBox alignment="CENTER">
            <children>
                <Button mnemonicParsing="false" onAction="#update" text="Update" />
            </children>
        </VBox>
    </children>
</VBox>
