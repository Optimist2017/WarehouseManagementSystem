<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox prefHeight="374.0" prefWidth="296.0" spacing="10.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.addbrandController">
   <children>
      <VBox alignment="CENTER">
         <children>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Add Brand">
               <font>
                  <Font size="25.0" />
               </font>
            </Text>
         </children>
      </VBox>
      <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Supplied By :" />
      <ChoiceBox fx:id="supplier" prefHeight="25.0" prefWidth="332.0" />
      <Text layoutX="10.0" layoutY="57.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Brand Name :" />
      <TextField fx:id="name" />
      <Text layoutX="10.0" layoutY="74.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Description :" />
      <TextArea fx:id="description" prefHeight="122.0" prefWidth="489.0" />
      <VBox alignment="CENTER">
         <children>
            <Button mnemonicParsing="false" onAction="#save" text="Save" />
         </children>
      </VBox>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
